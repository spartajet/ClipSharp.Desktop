<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1600" d:DesignHeight="450"
        x:Class="ClipSharp.Core.Views.DisplayWindow" WindowStartupLocation="CenterScreen"
        Title="DisplayWindow" CanResize="False" ShowInTaskbar="False">
    <Window.Styles>
        <Style Selector="Button" >
            <Setter Property="Height" Value="60"></Setter>
            <Setter Property="Width" Value="120"></Setter>
            <Setter Property="FontSize" Value="30"></Setter>
            <Setter Property="Margin" Value="5"></Setter>
        </Style>
    </Window.Styles>
    <Grid RowDefinitions="*,100">
        <StackPanel Grid.Row="1" >
            <Button Content="查找" Click="SearchButton_OnClick"></Button>
            <Button Content="Text"></Button>
            <Button Content="Image"></Button>
            <Button Content="Code"></Button>
            <Button Content="File"></Button>
        </StackPanel>
    </Grid>
</Window>